{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "test:all-safe": "vitest run --no-threads --reporter=verbose --pool=forks --poolOptions.forks.singleFork=true",
    "test:groups": "npm run test:group1 && npm run test:group2 && npm run test:group3",
    "test:group1": "vitest run src/components/__tests__/Register.test.jsx src/components/__tests__/Login.test.jsx --no-threads",
    "test:group2": "vitest run src/components/__tests__/AudioPlayer.test.jsx src/components/__tests__/Home.test.jsx --no-threads",
    "test:group3": "vitest run src/components/__tests__/Account.test.jsx src/components/__tests__/YourLibrary.test.jsx --no-threads",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "@react-oauth/google": "^0.12.2",
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.3.1",
    "axios": "^1.9.0",
    "lucide-react": "^0.518.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.57.0",
    "react-router-dom": "^7.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.7",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jsdom": "^22.1.0",
    "msw": "^1.3.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.0",
    "vite": "^6.3.5",
    "vitest": "^0.34.6"
  }
}
